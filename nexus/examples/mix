#NEXUS 

Begin data;
Dimensions ntax=4 nchar=2;
Format datatype=standard symbols="01" gap=-;
Matrix
Harry              00
Simon              01
Betty              10
Louise             11
    ;
End;
#nexus

BEGIN Taxa;
DIMENSIONS ntax=4;
TAXLABELS
[1] 'John'
[2] 'Paul'
[3] 'George'
[4] 'Ringo'
;
END; [Taxa]

BEGIN Characters;
DIMENSIONS nchar=4;
FORMAT
	datatype=DNA
	missing=?
	gap=-
	symbols="atgc"
	labels=left
	transpose=no
	interleave=yes
;
MATRIX
'John'     actg
'Paul'     actg
'George'   actg
'Ringo'    actg
;
END; [Characters]
#NEXUS 
[interleaved! ]

Begin data;
Dimensions ntax=2 nchar=6;
Format datatype=standard symbols="012345" gap=- interleave;
Matrix
Harry              01
Simon              01

Harry              23
Simon              23
Harry              45
Simon              45
    ;
End;
